{
	"manifest_version": 2,
	
	"name": "GTYST Scheduller Retriever",
	"description": "This extension will read the schedule data from a page.",
	"version": "1.0",
	
	"browser_action":
	{
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	
	"content_scripts": [
	{
		"matches": ["http://*/*", "https://*/*", "http://webadvisor.uoguelph.ca/*"],
		"js": [ "jquery.min.js", "content.js" ]
	}],
	
	"background": 
	{
		"persistent": false,
		"scripts": [ "background.js" ]
	},
	
	/*"content_security_policy": "script-src https://ajax.googleapis.com 'self'; object-src 'self'",*/
	"permissions": ["activeTab"]
}